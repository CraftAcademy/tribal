<%= form_with url: transactions_path, local: true, class:"main_container", id: 'transaction_form' do |form| %>
	<%= form.hidden_field :session_id, value: @session.id %>
  <div class="flex flex-col items-center m-4">
		<%= form.label 'Credit or debit card',  class: 'm-4 text-3xl' %>
		<div class="bg-white border-2 w-1/3 p-2 text-xl m-4 text-center" id="card-element"></div>
	  <%= form.submit 'Submit Payment', class: 'button bg-transparent m-4' %>
	</div>
<% end %>
